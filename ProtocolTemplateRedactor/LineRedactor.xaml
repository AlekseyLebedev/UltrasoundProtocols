<UserControl x:Class="ProtocolTemplateRedactor.LineRedactor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProtocolTemplateRedactor"
             mc:Ignorable="d" 
             x:ClassModifier="internal"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TextBox x:Name="labelTextBox" Height="23" Margin="10,40.96,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" TextChanged="labelTextBox_TextChanged"/>
        <Label Content="Подпись к строке: " Margin="10,10,10,0" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
