<UserControl x:Class="ProtocolTemplateRedactor.ComboBoxRedactor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProtocolTemplateRedactor"
             mc:Ignorable="d" 
             x:ClassModifier="internal"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <CheckBox x:Name="OtherEnabled" Content="Доступно другое значение" Margin="10,83.683,10,0" VerticalAlignment="Top" Checked="OtherEnabled_Checked"/>
        <TextBlock Margin="10,0,0,10" TextWrapping="Wrap" Text="Введите элементы по одному на строку" VerticalAlignment="Bottom" HorizontalAlignment="Left" Width="280"/>
        <TextBox x:Name="VariantsTextBox" AcceptsReturn="True" Margin="10,103.782,10,30.96" TextWrapping="Wrap" Text="" TextChanged="VariantsTextBox_TextChanged"/>
        <local:LineRedactor x:Name="line" Margin="0,10,0,0" VerticalAlignment="Top" Height="68.683"/>
    </Grid>
</UserControl>
